		* txtFont.Left = 202
		* chkOverrideCharset.Left = 202
		* txtPrintDevice.Left = 202
		* txtPrintDevice.Width = 316
		* cmd1.Left = 202
		* lbl2.Left = 201
		* lbl2.Top = 208
		
		* Added cmdSelectPrinter so user can select the desired printer in Vista and above.
		
		* Also, Init sets the Caption, Name, and HelpContextID of the tab.
		
	ENDPROC

	PROCEDURE Init
		This.Caption       = UI_TAB_PAGELAYOUT_LOC
		This.Name          = 'pageLayout'
		This.HelpContextID = UI_REPORT_PROPS_PAGELAYOUT_HELP_ID
		
	ENDPROC

	PROCEDURE Panelpagelayout1.cmdSelectPrinter.Click
		* Thanks for Francis Faure for this code.
		
		local lcVFPPrinter, ;
			loExprReader, ;
			lcDevice, ;
			laListPrinters[1], ;
			lcPrint, ;
			lnCurSel
		
		* Save the current VFP printer .
		
		lcVFPPrinter = set('PRINTER', 3)
		
		* Get the printer saved with the report and its device capabilities.
		
		loExprReader = newobject('NameValuePairManager', FRXCOMMON_PRG_CLASSLIB)
		loExprReader.LoadMemo(EXPR)
		lcDevice = loExprReader.Get('DEVICE')
		
		* Set the VFP printer to the report printer (if there is one and it exists).
		
		if not empty(lcDevice)
			aprinters(laListPrinters, 0)
			if ascan(laListPrinters, lcDevice) > 0
				set printer to name (lcDevice)
			endif ascan(laListPrinters, lcDevice) > 0
		endif not empty(lcDevice)
		
		* Let the user choose the printer. If one was selected, make it the current
		* one, save it in the FRX, and check the Save Printed Environment checkbox.
		
		lcPrint = getprinter()
		if not empty(lcPrint)
			This.Parent.txtPrintDevice.Value = lcPrint
			This.Parent.bufferIsDirty = .T.
			set printer to name (lcPrint)
			if used(This.Parent.Tag)
				lnCurSel = select()
				select (This.Parent.Tag)
				locate for OBJTYPE = FRX_OBJTYP_REPORTHEADER
				if found()
					replace TAG with '', TAG2 with ''
					sys(1037,2)
					This.Parent.ResynchPanel()
				endif found()
				select (lnCurSel)
			endif used(This.Parent.Tag)
			This.Parent.chkPrintEnvironment.Value = .T.
		endif not empty(lcPrint)
		
		* Restore the VFP printer
		
		set printer to name (lcVFPPrinter)
		
	ENDPROC

ENDDEFINE

