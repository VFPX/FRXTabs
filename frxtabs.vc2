		#IF USE_LOC_STRINGS_IN_UI
			This.Caption = UI_APPLY_ROTATION_LOC
		#ENDIF
		
	ENDPROC

	PROCEDURE Panelmultirotate1.loadfromfrx
		local llSameAngle, ;
			lnRecno, ;
			llFirst, ;
			lnAngle
		dodefault()
		with This
			llSameAngle = .T.
		
		* Go through each selected object and see which ones have the same settings.
		
			select FRX
			lnRecno = recno()
			llFirst = .T.
			scan for CURPOS and recno() > 1 and OBJTYPE <> FRX_OBJTYP_GROUP
				.FrxCursor.UnpackMemberData()
				select MEMBERDATA
				locate for TYPE = FRX_BLDR_MEMBERDATATYPE and ;
					NAME = FRX_BLDR_NAMESPACE_ROTATE
				if llFirst
					lnAngle = int(val(alltrim(EXECUTE)))
				else
					llSameAngle = llSameAngle and int(val(alltrim(EXECUTE))) = lnAngle
				endif llFirst
		
		* Flag that we're no longer on the first record.
		
				llFirst = .F.
				select FRX
			endscan for CURPOS ...
			Thisform.frxEvent.frxCursor.goRec(lnRecno, 'FRX')
		
		* Set the controls.
		
			if llSameAngle
				.PanelRotate.spnAngle.Value = lnAngle
				.PanelRotate.RotatePreview.SetAngle(lnAngle)
			endif llSameAngle
		endwith
		
	ENDPROC

	PROCEDURE Panelmultirotate1.panelrotate.lblAngle.Init
		#IF USE_LOC_STRINGS_IN_UI
			This.Caption = UI_ANGLE_LOC
		#ENDIF
		
	ENDPROC

