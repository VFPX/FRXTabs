	PROCEDURE gethandlerloadfromfrxscript		&& Gets the handler's LoadFromFRX script from the Registry table
		* Add the include files the script code needs.
		
		lparameters tcCode, ;
			taFiles
		local lcCode, ;
			lcNewCode, ;
			lnFiles, ;
			lnI, ;
			lcInclude, ;
			lcFile, ;
			lcIncludeCode
		lcCode    = strtran(tcCode, chr(10))
		lcNewCode = tcCode
		if pcount() = 1
			lnFiles = 0
			dimension taFiles[1]
		else
			lnFiles = alen(taFiles)
		endif pcount() = 1
		for lnI = 1 to occurs(chr(13) + '#include', lower(lcCode))
			lcInclude = strextract(lcCode, chr(13) + '#include ', chr(13), lnI, 1)
			lcFile    = upper(juststem(lcInclude))
			if not empty(lcInclude) and file(lcInclude) and ascan(taFiles, lcFile) = 0
				lnFiles = lnFiles + 1
				dimension taFiles[lnFiles]
				taFiles[lnFiles] = lcFile
				lcIncludeCode    = This.GetHandlerLoadFromFRXScript(filetostr(lcInclude), ;
					@taFiles)
				lcNewCode        = lcIncludeCode + chr(13) + lcNewCode
			endif not empty(lcInclude) ...
			lcNewCode = strtran(lcNewCode, '#include ' + lcInclude)
		next lnI
		return lcNewCode
		
	ENDPROC

	PROCEDURE KeyPress
		* The parent method can change the active tab. Frame.Click updates the tab as
		* it changes, but that won't fire when we do it this way, so force the code to
